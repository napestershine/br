        <!-- j Query -->
        <script type="text/javascript" src="{{ asset('reweb/js/jquery-2.1.4.js') }}"></script>

        <!-- Bootstrap JS -->
        <script type="text/javascript" src="{{ asset('reweb/js/bootstrap.min.js') }}"></script>



        <!-- Tooltip -->
        <script type="text/javascript" src="{{ asset('reweb/js/jquery.tooltipster.min.js') }}"></script>

        <!-- ui js -->
        <script type="text/javascript" src="{{ asset('reweb/js/jquery-ui.min.js') }}"></script>

        <!-- Fancybox js -->
        <script type="text/javascript" src="{{ asset('reweb/js/jquery.fancybox.pack.js') }}"></script>

        <!-- owl.carousel -->
        <script type="text/javascript" src="{{ asset('reweb/js/owl.carousel.min.js') }}"></script>

        <!-- revolution -->
        <script src="{{ asset('reweb/js/revolution/jquery.themepunch.tools.min.js') }}"></script>
        <script src="{{ asset('reweb/js/revolution/jquery.themepunch.revolution.min.js') }}"></script>
        <script type="text/javascript"
                src="{{ asset('reweb/js/revolution/revolution.extension.slideanims.min.js') }}"></script>
        <script type="text/javascript"
                src="{{ asset('reweb/js/revolution/revolution.extension.layeranimation.min.js') }}"></script>
