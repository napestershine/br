    <!-- Color css -->
    <link rel="stylesheet" type="text/css" href="{{ asset('reweb/css/responsive/responsive.css') }}"/>
</head>
<body class="{% block bodyclass %}home4{% endblock %}">
<div class="boxed_wrapper">
    {% block header %}
        <!-- Main Menu ____________________________________ -->
        <div class="main_menu menu_style_three menu_fixed_style_two bg-black-transparent" id="menu">
            <div class="container">
                <div class="wrapper">
                    <div class="logo_holder"><h2 style="color:#fff;">Brooter</h2></div>
                    <!-- End .logo_holder -->

                    <a href="#" class="amenities tran3s">Add Property</a>

                    <!-- Menu ______________ -->
                    <nav class="navbar navbar-default">
                        <!-- Brand and toggle get grouped for better mobile display -->
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse"
                                    data-target="#navbar-collapse-1" aria-expanded="false">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                        </div> <!-- End .navbar-header -->
                        <!-- Collect the nav links, forms, and other content for toggling -->
                        <div class="collapse navbar-collapse" id="navbar-collapse-1">
                            <ul class="nav navbar-nav">
                                <li class="tran3sL"><a href="{{ path('brooter_web_homepage') }}">Home</a></li>
                                <li class="tran3sL"><a href="">Properties</a></li>
                                <li class="tran3sL"><a href="{{ path('brooter_web_faq') }}">FAQs</a></li>
                                <li class="tran3sL"><a href="{{ path('brooter_blog_homepage') }}">Blog</a></li>
                                {% if app.user %}
                                    {% if is_granted('ROLE_ADMIN') %}
                                        <li class="tran3sL"><a href="{{ path('brooter_admin_homepage') }}">Admin</a></li>
                                    {% endif %}
                                    <li class="tran3sL"><a href="{{ path('fos_user_profile_show') }}">Profile</a></li>
                                    <li class="tran3sL"><a href="{{ path('fos_user_security_logout') }}">Logout</a></li>
                                {% else %}
                                    <li class="tran3sL"><a href="{{ path('fos_user_security_login') }}">Login</a></li>
                                    <li class="tran3sL"><a
                                                href="{{ path('fos_user_registration_register') }}">Register</a></li>
                                {% endif %}
                                <li class="tran3sL"><a href="">Contact Us</a></li>
                            </ul>
                        </div><!-- /.navbar-collapse -->
                    </nav> <!-- End .navbar -->
                    <!-- End Menu __________ -->
                </div>  <!-- End .wrapper -->
            </div> <!-- End .container -->
        </div> <!-- End .main_menu -->
        <!-- End Main Menu _______________________________ -->


        {% block mainBanner %}

        {% endblock %}

        <!-- End Main Banner______________________________ -->
    {% endblock %}
    {% block blogheader %}
    {% endblock %}
    <div class="{% block margintop %}margin-top-120{% endblock %}">
        {% block content %}
            <!-- Content Section -->
            <!-- End Content Section -->
        {% endblock %}
    </div>
    {% block footer %}
        <!-- Footer __________________________________________ -->
        <footer>
            <div class="main_footer">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12">
                            <div class="about_estate">
                                <h2 style="color:#fff;">Brooter</h2>
                                <p>{{ company.field11 }}</p>

                                <ul class="icon_footer">
                                    <li class="border_round tran3s tooltip" title="Facebook"><a
                                                href="{{ company.field6 }}"><i
                                                    class="fa fa-facebook"></i></a></li>
                                    <li class="border_round tran3s tooltip" title="Twitter"><a
                                                href="{{ company.field7 }}"><i
                                                    class="fa fa-twitter"></i></a></li>
                                    <li class="border_round tran3s tooltip" title="Google Plus"><a
                                                href="{{ company.field8 }}"><i
                                                    class="fa fa-google-plus"></i></a></li>
                                    <li class="border_round tran3s tooltip" title="Linkedin"><a
                                                href="{{ company.field9 }}"><i
                                                    class="fa fa-linkedin"></i></a></li>
                                    <li class="border_round tran3s tooltip" title="Vimeo"><a
                                                href="{{ company.field10 }}"><i
                                                    class="fa fa-vimeo"></i></a></li>
                                </ul> <!-- End .icon_header -->

                            </div> <!-- End .about_estate -->
                        </div>

                        <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12">
