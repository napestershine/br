{% extends 'admin.html.twig' %}
{% block header %} Area In{% endblock %}
{% block subheader %} New Area Unit{% endblock %}
{% block content %}
    <div class="panel-body">
        <div class="form-horizontal">
            {{ form_start(form) }}
            <div class="form-group">
                <div class="col-md-3">Area Unit Name</div>
                <div class="col-md-6">
                    {{ form_widget(form.areaInName) }}
                </div>
            </div>
            <br/>
            <div class="form-group">
                <div class="col-md-2">

                    <input type="submit" value="Create" class="btn btn-success"/>
                    <a class="btn btn-primary" href="{{ path('areain_index') }}">Back</a>
                </div>
            </div>
            {{ form_end(form) }}
        </div>
    </div>

{% endblock %}
