{% extends 'admin.html.twig' %}
{% block header %}Area In {% endblock %}
{% block subheader %}Show Area Unit Details {% endblock %}
{% block content %}
    <h1>AreaIn</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ areaIn.id }}</td>
            </tr>
            <tr>
                <th>Areainname</th>
                <td>{{ areaIn.areaInName }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('areain_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('areain_edit', { 'id': areaIn.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
