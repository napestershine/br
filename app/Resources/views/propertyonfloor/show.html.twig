{% extends 'admin.html.twig' %}
{% block header %} Property on Floor {% endblock %}
{% block subheader %} Property on Floor Details{% endblock %}
{% block content %}

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ propertyOnFloor.id }}</td>
            </tr>
            <tr>
                <th>Propfloor</th>
                <td>{{ propertyOnFloor.propFloor }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('propertyonfloor_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('propertyonfloor_edit', { 'id': propertyOnFloor.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
