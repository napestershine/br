{% extends 'admin.html.twig' %}
{% block header %}Property Advertisement For {% endblock %}
{% block subheader %} New Property Advertisement Purpose {% endblock %}
{% block content %}
    <div class="panel-body">
        <div class="form-horizontal">
            {{ form_start(form) }}
            <div class="form-group">
                <div class="col-md-3">Type of Availability</div>
                <div class="col-md-6">
                    {{ form_widget(form.listPropForName) }}
                </div>
            </div>
            <br/>
            <div class="form-group">
                <div class="col-md-2">
                    <input type="submit" value="Create" class="btn btn-success"/>
                    <a class="btn btn-primary"  href="{{ path('listpropfor_index') }}">Back</a>
                </div>
            </div>
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}
