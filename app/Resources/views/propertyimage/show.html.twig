{% extends 'base.html.twig' %}

{% block body %}
    <h1>PropertyImage</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ propertyImage.id }}</td>
            </tr>
            <tr>
                <th>Filepath</th>
                <td>{{ propertyImage.filePath }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('propertyimage_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('propertyimage_edit', { 'id': propertyImage.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
