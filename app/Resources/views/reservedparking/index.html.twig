{% extends 'admin.html.twig' %}
{% block header %} Reserved Parking {% endblock %}
{% block subheader %} Reserved Parking with Property{% endblock %}
{% block content %}
    <div class="row">
        <div class="col-md-1">
            <a href="{{ path('reservedparking_new') }}" class="btn btn-success">Add New</a>
        </div>
    </div>
    <br/>
    <div class="table table-responsive">
        <table class="table table-bordered">
            <thead>
            <tr>
                <th>Sr. No.</th>
                <th>Reserved Parking Type</th>
                <th>Actions</th>
            </tr>
            </thead>
            <tbody>
            {% for reservedParking in reservedParkings %}
                <tr>
                    <td>{{ loop.index }}</td>
                    <td>{{ reservedParking.reservedParkingType }}</td>
                    <td>
                        <a class="btn btn-info" href="{{ path('reservedparking_show', { 'id': reservedParking.id }) }}">show</a>

                        <a class="btn btn-primary" href="{{ path('reservedparking_edit', { 'id': reservedParking.id }) }}">edit</a>

                    </td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
{% endblock %}