{% extends 'base.html.twig' %}

{% block body %}
    <h1>PropertySpecification</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ propertySpecification.id }}</td>
            </tr>
            <tr>
                <th>Propspecificationtitle</th>
                <td>{{ propertySpecification.propSpecificationTitle }}</td>
            </tr>
            <tr>
                <th>Propspecdesc</th>
                <td>{{ propertySpecification.propSpecDesc }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('propertyspecification_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('propertyspecification_edit', { 'id': propertySpecification.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
